package flex;

public class pac {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str="Saheli";
		int c;
		for(int i=0;i<str.length();i++) 
		{
			c=1;
			for(int j=i+1;j<str.length();j++) 
			{
				char a=str.charAt(i);
				char b=str.charAt(j);
				if(a==b) {
					c++;
				}
			}
		}
		
		
	}

}
